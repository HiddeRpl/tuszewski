  "scripts": {
    "build:prod": "yarn fix && webpack --mode production --config webpack.config.js --progress --colors",
    "build": "webpack --mode development --config webpack.config.js --progress --colors",
    "watch": "webpack --mode development --config webpack.config.js --progress --colors --watch",
    "serve": "webpack-dev-server",
    "serve:prod": "NODE_ENV=production webpack-dev-server",
    "lint": "tslint -p ./tsconfig.json -t codeFrame",
    "prettier": "prettier \"./**/*.{ts,tsx,scss}\" --write",
    "fix:ts": "yarn lint --fix",
    "fix": "yarn prettier && yarn fix:ts",
    "deploy": "yarn run build:prod && gh-pages -d dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^10.12.18",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "node-sass": "^4.12.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
